{
  "manifest_version": 3,
  "name": "Lock In",
  "version": "1.0",
  "description": "Blocks distractions like YouTube Shorts, Instagram Reels.",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["src/contentScript.js"],
      "run_at": "document_start"
    }
  ]
}
